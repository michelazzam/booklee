{
  "cli": {
    "version": ">= 16.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development"
    },
    "preview": {
      "channel": "preview",
      "autoIncrement": true,
      "distribution": "store",
      "environment": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production",
      "environment": "production"
    }
  },
  "submit": {
    "production": {},
    "preview": {
      "ios": {
        "bundleIdentifier": "app.booklee.preview"
      }
    }
  }
}
