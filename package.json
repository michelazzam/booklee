{
  "name": "booklee",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "android": "expo run:android",
    "build:prod": "eas build --profile production --platform all",
    "build:dev": "eas build --profile development --platform all",
    "update:dev": "eas update --branch development --platform all",
    "update:prod": "eas update --branch production --platform all",
    "submit:prod": "eas submit --profile production --platform all",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:prod:ios": "eas build --profile production --platform ios",
    "update:dev:ios": "eas update --branch development --platform ios",
    "update:prod:ios": "eas update --branch production --platform ios",
    "submit:prod:ios": "eas submit --profile production --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:dev:android": "eas build --profile development --platform android",
    "update:prod:android": "eas update --branch production --platform android",
    "update:dev:android": "eas update --branch development --platform android",
    "submit:prod:android": "eas submit --profile production --platform android",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write"
  },
